    {
      "parameters": {
        "jsCode": "try {\n  const item = $input.first();\n  const json = item?.json || {};\n  const data = json.data || json;\n  const textRaw = (data.text || data.message || '').toString();\n  const text = textRaw.trim().toLowerCase();\n\n  let intent = 'unknown';\n  if (!text) intent = 'empty';\n  else if (['/book','/agendar','agendar','reservar','book'].includes(text)) intent = 'booking';\n  else if (['/availability','/disponibilidad','disponibilidad','horarios','horas','slots'].includes(text)) intent = 'availability';\n  else if (['/cancel','/cancelar','cancelar','anular'].includes(text)) intent = 'cancel';\n  else if (['/reschedule','/reprogramar','reprogramar','cambiar hora'].includes(text)) intent = 'reschedule';\n  else if (['/admin','/config','/providers','/dashboard'].includes(text)) intent = 'admin';\n  else if (['/help','/ayuda','ayuda','faq'].includes(text)) intent = 'help';\n\n  return [{ json: { ...json, data: { ...data, text: textRaw, intent } } }];\n} catch (e) {\n  return [{ json: { success: false, error_code: 'INTENT_NORMALIZE_FAIL', error_message: e.message, data: null } }];\n}"
      },
