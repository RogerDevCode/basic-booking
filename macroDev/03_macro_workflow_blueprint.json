        "jsCode": "try {\n  const item = $input.first();\n  if (!item || !item.json) {\n    return [{ json: { success: false, error_code: 'NO_INPUT', error_message: 'Payload vacío', data: null } }];\n  }\n  const input = item.json.body || item.json;\n  return [{ json: { success: true, error_code: null, error_message: null, data: input } }];\n} catch (e) {\n  return [{ json: { success: false, error_code: 'GUARD_EXCEPTION', error_message: e.message, data: null } }];\n}"
      },
